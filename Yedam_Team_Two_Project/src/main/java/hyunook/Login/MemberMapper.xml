<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hyunook.Login.MemberMapper">
	<select id="selectMember" parameterType="common.Member" resultType="common.Member">
		SELECT member_id,name,member_type,password
		FROM member
		WHERE member_id= #{memberId }
		and password= #{password }
	</select>

	<!-- <insert id="insertMember" parameterType="member">
		INSERT INTO tbl_member(id,pw,name, image)
		values (#{id },#{pw },#{name },#{image })
	</insert> -->
</mapper>